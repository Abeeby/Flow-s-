{
  "version": 2,
  "framework": "nextjs",
  "buildCommand": "npm install && prisma generate && next build",
  "installCommand": "npm install",
  "outputDirectory": ".next",
  "env": {
    "NODE_ENV": "production",
    "DATABASE_URL": "${DATABASE_URL}"
  },
  "rewrites": [
    { "source": "/(.*)", "destination": "/" }
  ]
} 